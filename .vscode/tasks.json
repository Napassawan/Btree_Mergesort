{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: Build generator_new (Meson)",
			"command": "meson",
			"args": [
				"compile",
				"generator_new"
			],
			"options": {
				"cwd": "${workspaceFolder}/GNUBenchmark_uint32_generator/build/"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		}
	]
}